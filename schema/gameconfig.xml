<?xml version="1.0"?>
<schema version="0.3">
  <table name="gameconfig">
    <field name="config_id" type="I" size="5">
      <KEY/>
      <AUTOINCREMENT/>
    </field>
    <field name="section" type="C" size="30">
      <NOTNULL/>
      <DEFAULT value="game"/>
    </field>
    <field name="name" type="C" size="75">
      <NOTNULL/>
    </field>
    <field name="category" type="C" size="30">
      <NOTNULL/>
    </field>
    <field name="value" type="C" size="128">
      <NOTNULL/>
    </field>
    <field name="description" type="C" size="255">
      <NOTNULL/>
    </field>
  </table>
</schema>
