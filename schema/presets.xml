<?xml version="1.0"?>
<schema version="0.2">
    <table name="presets">
        <field name="preset_id" type="I4">
            <key/>
            <autoincrement/>
        </field>
        <field name="player_id" type="I2"><notnull/><default value="0"/></field>
        <field name="preset" type="I4"><notnull/><default value="1"/></field>
        <field name="type" type="C" size="1"><notnull/><default value="R"/></field>
        <index name="presets_player_id">
            <col>player_id</col>
        </index>
    </table>
</schema>
