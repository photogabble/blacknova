<?xml version="1.0"?>
<schema version="0.3">
  <table name="sector_defence">
    <field name="defence_id" type="I" size="10">
      <KEY/>
      <AUTOINCREMENT/>
      <UNSIGNED/>
    </field>
    <field name="ship_id" type="I" size="11">
      <NOTNULL/>
      <DEFAULT value="0"/>
    </field>
    <field name="sector_id" type="I" size="10">
      <NOTNULL/>
      <DEFAULT value="0"/>
      <UNSIGNED/>
    </field>
    <field name="defence_type" type="C" size="1">
      <NOTNULL/>
      <DEFAULT value="M"/>
    </field>
    <field name="quantity" type="I" size="20">
      <NOTNULL/>
      <DEFAULT value="0"/>
    </field>
    <field name="fm_setting" type="C" size="6">
      <NOTNULL/>
      <DEFAULT value="toll"/>
    </field>
    <index name="sector_id">
      <col>sector_id</col>
    </index>
    <index name="ship_id">
      <col>ship_id</col>
    </index>
  </table>
</schema>
