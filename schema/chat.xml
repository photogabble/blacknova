<?xml version="1.0"?>
<schema version="0.2">
    <table name="chat">
        <desc>Interactive chat system</desc>
        <field name="chat_id" type="I8">
            <key/>
            <autoincrement/>
        </field>
        <field name="chat_sender" type="I2"></field>
        <field name="chat_channel" type="I2"></field>
        <field name="chat_text" type="C" size="80"></field>
    </table>
</schema>
