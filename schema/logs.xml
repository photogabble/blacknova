<?xml version="1.0"?>
<schema version="0.3">
  <table name="logs">
    <field name="log_id" type="I" size="10">
      <KEY/>
      <AUTOINCREMENT/>
      <UNSIGNED/>
    </field>
    <field name="ship_id" type="I" size="11">
      <NOTNULL/>
      <DEFAULT value="0"/>
    </field>
    <field name="type" type="I" size="5">
      <NOTNULL/>
      <DEFAULT value="0"/>
    </field>
    <field name="time" type="T"/>
    <field name="data" type="X"/>
    <index name="idate">
      <col>ship_id</col>
      <col>time</col>
    </index>
  </table>
</schema>
