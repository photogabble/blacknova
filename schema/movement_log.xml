<?xml version="1.0"?>
<schema version="0.3">
  <table name="movement_log">
    <field name="event_id" type="I" size="10">
      <KEY/>
      <AUTOINCREMENT/>
      <UNSIGNED/>
    </field>
    <field name="ship_id" type="I" size="11">
      <NOTNULL/>
      <DEFAULT value="0"/>
    </field>
    <field name="sector_id" type="I" size="11">
      <DEFAULT value="0"/>
    </field>
    <field name="time" type="T"/>
    <index name="ship_id">
      <col>ship_id</col>
    </index>
    <index name="sector_id">
      <col>sector_id</col>
    </index>
  </table>
</schema>
