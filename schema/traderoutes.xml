<?xml version="1.0"?>
<schema version="0.3">
  <table name="traderoutes">
    <field name="traderoute_id" type="I" size="10">
      <KEY/>
      <AUTOINCREMENT/>
      <UNSIGNED/>
    </field>
    <field name="source_id" type="I" size="10">
      <NOTNULL/>
      <DEFAULT value="0"/>
      <UNSIGNED/>
    </field>
    <field name="dest_id" type="I" size="10">
      <NOTNULL/>
      <DEFAULT value="0"/>
      <UNSIGNED/>
    </field>
    <field name="source_type" type="C" size="1">
      <NOTNULL/>
      <DEFAULT value="P"/>
    </field>
    <field name="dest_type" type="C" size="1">
      <NOTNULL/>
      <DEFAULT value="P"/>
    </field>
    <field name="move_type" type="C" size="1">
      <NOTNULL/>
      <DEFAULT value="W"/>
    </field>
    <field name="owner" type="I" size="10">
      <NOTNULL/>
      <DEFAULT value="0"/>
      <UNSIGNED/>
    </field>
    <field name="circuit" type="C" size="1">
      <NOTNULL/>
      <DEFAULT value="2"/>
    </field>
    <index name="owner">
      <col>owner</col>
    </index>
  </table>
</schema>
