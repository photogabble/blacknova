<?xml version="1.0"?>
<schema version="0.3">
  <table name="scheduler">
    <field name="sched_id" type="I" size="10">
      <KEY/>
      <AUTOINCREMENT/>
      <UNSIGNED/>
    </field>
    <field name="repeat" type="C" size="1">
      <NOTNULL/>
      <DEFAULT value="N"/>
    </field>
    <field name="ticks_left" type="I" size="10">
      <NOTNULL/>
      <DEFAULT value="0"/>
      <UNSIGNED/>
    </field>
    <field name="ticks_full" type="I" size="10">
      <NOTNULL/>
      <DEFAULT value="0"/>
      <UNSIGNED/>
    </field>
    <field name="spawn" type="I" size="10">
      <NOTNULL/>
      <DEFAULT value="0"/>
      <UNSIGNED/>
    </field>
    <field name="sched_file" type="C" size="30">
      <NOTNULL/>
    </field>
    <field name="extra_info" type="C" size="50"/>
    <field name="last_run" type="I" size="20"/>
  </table>
</schema>
