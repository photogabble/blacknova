<?xml version="1.0"?>
<schema version="0.3">
  <table name="universe">
    <field name="sector_id" type="I" size="10">
      <KEY/>
      <AUTOINCREMENT/>
      <UNSIGNED/>
    </field>
    <field name="sector_name" type="C" size="30"/>
    <field name="zone_id" type="I" size="11">
      <NOTNULL/>
      <DEFAULT value="0"/>
    </field>
    <field name="port_type" type="C" size="8">
      <NOTNULL/>
      <DEFAULT value="none"/>
    </field>
    <field name="port_organics" type="I" size="20">
      <NOTNULL/>
      <DEFAULT value="0"/>
    </field>
    <field name="port_ore" type="I" size="20">
      <NOTNULL/>
      <DEFAULT value="0"/>
    </field>
    <field name="port_goods" type="I" size="20">
      <NOTNULL/>
      <DEFAULT value="0"/>
    </field>
    <field name="port_energy" type="I" size="20">
      <NOTNULL/>
      <DEFAULT value="0"/>
    </field>
    <field name="beacon" type="C" size="50"/>
    <field name="angle1" type="N" size="10">
      <NOTNULL/>
      <DEFAULT value="0.00"/>
    </field>
    <field name="angle2" type="N" size="10">
      <NOTNULL/>
      <DEFAULT value="0.00"/>
    </field>
    <field name="distance" type="I" size="20">
      <NOTNULL/>
      <DEFAULT value="0"/>
      <UNSIGNED/>
    </field>
    <field name="fighters" type="I" size="20">
      <NOTNULL/>
      <DEFAULT value="0"/>
    </field>
    <index name="zone_id">
      <col>zone_id</col>
    </index>
    <index name="port_type">
      <col>port_type</col>
    </index>
  </table>
</schema>
